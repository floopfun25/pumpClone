    "@supabase/supabase-js": "^2.38.4",
    "@vueuse/core": "^10.5.0",
    "bs58": "^4.0.1",
    "chart.js": "^4.4.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.5.0",
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "pinia": "^2.1.7",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
