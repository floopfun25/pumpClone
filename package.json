{
  "name": "floppfun",
  "version": "1.0.0",
  "description": "FloppFun - A decentralized meme token launchpad on Solana",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "NODE_ENV=production vite build --mode production",
    "build:mainnet": "cp .env.production .env.local && npm run build:production",
    "build-with-types": "vue-tsc && vite build",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "test:integration": "npm run build:mainnet && npm run preview:production",
    "docker:build": "docker build -t pump-fun-clone .",
    "docker:run": "docker run -p 3000:3000 pump-fun-clone",
    "deploy:setup": "bash deploy/setup.sh",
    "deploy:configure": "bash deploy/configure.sh",
    "deploy:build": "bash deploy/build.sh",
    "deploy:test": "bash deploy/test-deploy.sh",
    "deploy:mainnet": "bash deploy-mainnet.sh",
    "deploy:help": "cat MAINNET_DEPLOYMENT_GUIDE.md",
    "mainnet:prepare": "cp .env.production .env.local && npm run type-check && npm run lint",
    "mainnet:build": "npm run mainnet:prepare && npm run build:production",
    "mainnet:deploy": "npm run mainnet:build && bash deploy-mainnet.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.0",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@magic-ext/solana": "^26.1.0",
    "@metaplex-foundation/mpl-token-metadata": "^2.13.0",
    "@solana-mobile/wallet-adapter-mobile": "^2.2.0",
    "@solana/spl-token": "^0.3.9",
    "@solana/sysvars": "^2.1.0",
    "@solana/wallet-adapter-base": "^0.9.26",
    "@solana/wallet-adapter-phantom": "^0.9.27",
    "@solana/wallet-adapter-solflare": "^0.6.31",
    "@solana/wallet-adapter-vue": "^0.4.5",
    "@solana/wallet-adapter-wallets": "^0.19.36",
    "@solana/web3.js": "^1.98.2",
    "@vueuse/core": "^13.3.0",
    "borsh": "^0.7.0",
    "bs58": "^4.0.1",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.5.0",
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "lightweight-charts": "^5.0.7",
    "magic-sdk": "^29.1.0",
    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "recharts": "^2.15.3",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.6.2",
    "tweetnacl": "^1.0.3",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-i18n": "^9.14.4",
    "vue-router": "^4.2.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@rushstack/eslint-patch": "^1.5.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tsconfig/node18": "^18.2.2",
    "@types/bs58": "^4.0.4",
    "@types/node": "^20.9.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "postcss": "^8.5.4",
    "prettier": "^3.0.3",
    "supabase": "^1.180.0",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "vite": "^4.5.0",
    "vue-tsc": "^3.0.6"
  }
}
